<% title 'Challenges' %>

<div id="content">
<div class="wrap">

	<h2 class="banner"><%= @challenge.name %></h2>
	<h4><%= @challenge.start_date.strftime("%b %d") %> - <%= @challenge.end_date.strftime("%b %d") %></h4>

	<%= link_to 'Edit', edit_challenge_path(@challenge) %> |
	<%= link_to 'Back to Challenge Archive', challenges_path %>

</div>
</div>